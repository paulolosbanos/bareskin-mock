        "contains" : "xml"
      }
    },
    "bodyPatterns": [{
      "matchesXPath" : "//*[contains(local-name(), 'checkTrans')]"
    }]
  },
  "response" : {
    "status" : 200,
    "transformers": ["vn-epay-checktrans-transformer"]
  }
}